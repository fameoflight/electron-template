{
  "name": "codeblocks",
  "private": true,
  "version": "0.0.1",
  "description": "A batteries-included Electron template with React, TypeORM, GraphQL (Relay), and a robust job system.",
  "author": "Your Name <your.email@example.com>",
  "type": "module",
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": ">=14.x",
      "onFail": "error"
    }
  },
  "scripts": {
    "dev": "concurrently -k -n relay,vite \"yarn relay --watch\" \"vite\"",
    "graphql": "tsx cli/graphql-cli.js",
    "build": "tsx cli/utils-cli.ts build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "relay": "relay-compiler",
    "db:drop": "rm -rf .data",
    "db:reset": "rm -rf .data && yarn db:seed",
    "db:seed": "tsx cli/utils-cli.ts seed",
    "db:migrate": "tsx cli/utils-cli.ts migration:run",
    "db:rollback": "tsx cli/utils-cli.ts migration:revert",
    "migration:generate": "tsx cli/utils-cli.ts migration:generate",
    "migration:show": "tsx cli/utils-cli.ts migration:show",
    "migration:run": "tsx cli/utils-cli.ts migration:run",
    "migration:revert": "tsx cli/utils-cli.ts migration:revert",
    "generate": "tsx cli/index.ts",
    "g": "tsx cli/index.ts",
    "console": "tsx cli/utils-cli.ts console",
    "utils": "tsx cli/utils-cli.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit",
    "clean": "tsx cli/utils-cli.ts clean",
    "setup:completions": "bash cli/completions/install.sh"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.42",
    "@ai-sdk/openai": "^2.0.64",
    "@ai-sdk/openai-compatible": "^1.0.26",
    "@ant-design/icons": "5.x",
    "@apollo/server": "^5.1.0",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/vite": "^4.1.14",
    "@testing-library/react": "^16.3.0",
    "@types/mime-types": "^3.0.1",
    "ai": "^5.0.89",
    "antd": "^5.27.5",
    "class-validator": "^0.14.2",
    "dataloader": "^2.2.3",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.24",
    "graphql": "^16.11.0",
    "graphql-scalars": "^1.25.0",
    "graphql-type-json": "^0.3.2",
    "is-hotkey": "^0.2.0",
    "lodash": "^4.17.21",
    "marked": "^17.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-relay": "^20.1.1",
    "react-router-dom": "^7.9.4",
    "react-syntax-highlighter": "^16.1.0",
    "reflect-metadata": "^0.2.2",
    "relay-runtime": "^20.1.1",
    "sqlite3": "^5.1.7",
    "type-graphql": "^2.0.0-rc.2",
    "typeorm": "^0.3.27",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.39.0",
    "@faker-js/faker": "^10.1.0",
    "@pollyjs/adapter-fetch": "^6.0.7",
    "@pollyjs/adapter-node-http": "^6.0.6",
    "@pollyjs/core": "^6.0.6",
    "@pollyjs/persister-fs": "^6.0.6",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/is-hotkey": "^0.1.10",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.9.1",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/react-relay": "^18.2.1",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/relay-runtime": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^4.0.6",
    "babel-plugin-relay": "^20.1.1",
    "commander": "^14.0.2",
    "concurrently": "^9.2.1",
    "electron": "^39.0.0",
    "electron-builder": "^24.13.3",
    "eslint": "^9.39.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "handlebars": "^4.7.8",
    "jsdom": "^27.0.1",
    "madge": "^8.0.0",
    "neo-blessed": "^0.2.0",
    "pluralize": "^8.0.0",
    "png-to-ico": "^3.0.1",
    "react-blessed": "^0.7.2",
    "relay-compiler": "^20.1.1",
    "setup-polly-jest": "^0.11.0",
    "sharp": "^0.34.4",
    "tailwindcss": "^4.1.14",
    "tree-kill": "^1.2.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.12",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-relay": "^2.1.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.5"
  },
  "main": "dist-electron/index.js",
  "build": {
    "mac": {
      "category": "public.app-category.productivity",
      "hardenedRuntime": true,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "gatekeeperAssess": false,
      "identity": null,
      "icon": "build/icons/icon.icns"
    },
    "dmg": {
      "title": "${productName} ${version}",
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ],
      "icon": "build/icons/icon.icns"
    },
    "npmRebuild": false,
    "files": [
      "build/**/*",
      "dist/**/*"
    ],
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons/linux/"
    }
  }
}
